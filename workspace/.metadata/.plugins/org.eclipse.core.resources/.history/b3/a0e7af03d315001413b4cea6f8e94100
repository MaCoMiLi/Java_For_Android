public class DumpArgs
{
	
	public static void main(String[] args)
	{
		
		int age;
		age = 5;
		
		switch(age)
		{
		
			case 1:
				System.out.println("1 yaşındasın");
				break;
			
			case 2:
				System.out.println("2 yaşındasın");
				break;
			
			case 3:
				System.out.println("3 yaşındasın");
				break;
			
			default:
				System.out.println("3 yaşından büyüksün");
				break;
		
		
		}
		
	}
	
}