import java.util.Scanner;



public class DumpArgs
{
	
	public static void main(String[] args)
	{
		
		Scanner inir = new Scanner(System.in);
		System.out.println(inir.nextLine());
	
		char harf = 'u'; // anlamsız
		int puan = 28;
	
		if (puan >= 90)
		{
			harf = 'A';
			System.out.println("Aldığın not;");
			System.out.println(puan);
			System.out.println("------------");
			System.out.println("Harfli karşılığı;");
			System.out.println(harf);
			System.out.println("Testi geçtin");
			System.out.println("------------");
		}
		
		else if (puan >= 60)
		{
			harf = 'B';
			System.out.println("Aldığın not;");
			System.out.println(puan);
			System.out.println("------------");
			System.out.println("Harfli karşılığı;");
			System.out.println(harf);
			System.out.println("------------");
			System.out.println("Bu testte gayet iyisin");
		}
		
		else if (puan >= 40)
		{
			harf = 'C';
			System.out.println("Aldığın not;");
			System.out.println(puan);
			System.out.println("------------");
			System.out.println("Harfli karşılığı;");
			System.out.println(harf);
			System.out.println("------------");
			System.out.println("Kötü değil fakat daha iyisinin yapabilirsin");
		}
		
		else if (puan >= 30 )
		{
			harf = 'D';
			System.out.println("Aldığın not;");
			System.out.println(puan);
			System.out.println("------------");
			System.out.println("Harfli karşılığı;");
			System.out.println(harf);
			System.out.println("------------");
			System.out.println("Kötü. Bu dersten kaldın");
		}
		
		else
		{
			harf = 'F';
			System.out.print("Aldığın not: ");
			System.out.println(puan);
			System.out.println("------------");
			System.out.print("Harfli karşılığı: ");
			System.out.println(harf);
			System.out.println("------------");
			System.out.println("Senin okul bitmez hacı");
		}
	}
}